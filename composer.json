{
	"name": "studiomitte/cart-count",
	"type": "typo3-cms-extension",
	"description": "Endpoint for EXT:cart cart information which can be retrieved by an AJAX request",
	"keywords": [
		"TYPO3",
		"extension",
		"cart",
		"count"
	],
	"homepage": "https://studiomitte.com",
	"authors": [
		{
			"name": "Georg Ringer",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^9.5|| ^10.1",
		"extcode/cart": "^6 || ^7"
	},
	"autoload": {
		"psr-4": {
			"StudioMitte\\CartCount\\": "Classes"
		}
	},
	"replace": {
		"typo3-ter/cart-count": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "cart_count"
		}
	}
}
